{
  "manifest_version": 2,
  "name": "Tab logger",
  "version": "1.0.2",
  "description": "Logs tab behavior",
  "background": {
    "persistent": true,
    "scripts": ["3party/jquery-1.11.1.js","exp/intro.js","bgMsgReceiver.js","log/browserTabs.js","log/numberTabs.js","log/loadingTime.js","exp/expBg.js"]
  },
  "browser_action": {
    "default_title": "Taobao Web Search Study"
  },
  "content_scripts": [
    {
      //"matches": ["http://*/*","https://*/*"],
      "matches": ["*://*.taobao.com/*","*://stanford.edu/~fangx/cgi-bin/alibaba/*","*://alicefang.com/*"], 
      "js":["3party/jquery-1.11.1.js","contentScript.js","exp/expCS.js","exp/inactive.js"],
      "css": ["exp/expStyle.css"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}