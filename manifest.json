{
  "manifest_version": 2,
  "name": "Taobao search study",
  "version": "1.0.9",
  "description": "Alibaba User Experience Design research study on Taobao search.",
  "background": {
    "persistent": true,
    "scripts": ["3party/jquery-1.11.1.js","log/pageCountTime.js","bgMsgReceiver.js","exp/intro.js","log/browserTabs.js","log/loadingTime.js","exp/expBg.js"]
  },
  "browser_action": {
    "default_title": "Taobao Search Study"
  },
  "content_scripts": [
    {
      "matches": ["*://*.taobao.com/*","*://stanford.edu/~fangx/cgi-bin/alibaba/*","*://*.tmall.com/*"], 
      "js":["3party/jquery-1.11.1.js","contentScript.js","exp/expCS.js"],
      "css": ["exp/expStyle.css"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*.taobao.com/*",
    "*://stanford.edu/~fangx/cgi-bin/alibaba/*",
    "*://*.tmall.com/*",
    "tabs",
    "webNavigation"
  ]
}